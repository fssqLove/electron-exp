{
  "name": "electron_demo",
  "version": "1.0.0",
  "description": "electron_demo",
  "main": ".webpack/main.built.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./build/scripts/start.js",
    "start:render": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.dev.js",
    "build": "cross-env NODE_ENV=development node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:pack": "cross-env NODE_ENV=production node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:dist": "cross-env NODE_ENV=production node ./build/scripts/build.js --config=./electron-builder.json5",
    "pack": "electron-builder --dir ",
    "dist": "electron-builder --config=./electron-builder.json5"
  },
  "author": "lsq",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "electron": "^13.0.1",
    "electron-builder": "^22.10.5",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "node-loader": "^2.0.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.1"
  }
}
