{
  "name": "electron_demo",
  "version": "1.0.0",
  "description": "electron_demo",
  "main": ".webpack/main/main.built.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./build/scripts/start.js",
    "start:main": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.main.js",
    "start:render": "cross-env NODE_ENV=development webpack serve --config ./build/configs/webpack.dev.js",
    "pack": "electron-builder --dir ",
    "dist": "electron-builder --config=./electron-builder.json5"
  },
  "author": "lsq",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^13.0.1",
    "electron-builder": "^22.10.5",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "node-loader": "^2.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
